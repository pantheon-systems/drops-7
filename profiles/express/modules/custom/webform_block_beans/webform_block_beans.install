<?php

/**
 * Implements hook_install().
 */
function webform_block_beans_install() {
  // Create a bean for each webform_block already on the site.

  $blocks = webform_block_beans_get_blocks();
  foreach ($blocks as $block) {
    _webform_block_beans_create_bean('client-block-' . $block->nid , $block->title);
  }
}
