{
  "name": "radix",
  "description": "Radix is a base theme for Drupal. It has Bootstrap, Sass, Gulp, BrowserSync and Font Awesome built-in.",
  "homepage": "http://radixtheme.org",
  "version": "7.3.3",
  "authors": [
    "Arshad Chummun <arshad@chummun.com>"
  ],
  "moduleType": "globals",
  "main": [
    "assets/scss/_radix.scss",
    "assets/js/radix.script.js"
  ],
  "keywords": [
    "drupal",
    "themes",
    "bootstrap",
    "sass"
  ],
  "license": "MIT",
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests",
    "includes",
    "kits",
    "templates",
    "logo.png",
    "radix.drush.inc",
    "radix.info",
    "screenshot.png",
    "template.php",
    "THEMING.md"
  ],
  "dependencies": {
    "bootstrap-sass": "^3.3.7",
    "font-awesome": "^4.7.0"
  }
}
